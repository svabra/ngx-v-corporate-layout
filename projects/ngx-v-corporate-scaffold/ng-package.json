{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ngx-v-corporate-scaffold",
  "assets": [
    "css/**",
    { "input": "src/lib/css", "glob": "**/*", "output": "css" },
    "assets/**",
    { "input": "src/lib/assets", "glob": "**/*", "output": "assets" },
    { "input": "src/lib/favicon.ico", "glob": "**/*", "output": "." }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  }
}